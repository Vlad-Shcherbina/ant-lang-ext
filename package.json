{
    "name": "ant-lang",
    "displayName": "ant-lang",
    "description": "Description of my extension",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:ant"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "ant",
                "aliases": [
                    "Ant brain",
                    "ant"
                ],
                "extensions": [
                    ".ant"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ant",
                "scopeName": "source.ant",
                "path": "./syntaxes/ant.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/vscode": "^1.67.0"
    }
}
